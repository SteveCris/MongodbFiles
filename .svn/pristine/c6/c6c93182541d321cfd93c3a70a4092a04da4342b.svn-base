package com.shangyong.thjdq.dto;

import java.io.Serializable;

/**
 * Created by zbb on 2019-03-14.
 */
public class UserInfoDto implements Serializable {

    private static final long serialVersionUID = 8517224852818805400L;
    /**
     * 姓名
     */
    private String	user_name;
    /**
     * 手机号
     */
    private String	phone;
    /**
     * 身份证号（遇到x统一转化为大写）
     */
    private String	id_card;
    /**
     * 性别，男/女
     */
    private String	gender;
    /**
     * 身份信息（1=工薪族、2=企业主、3=自由职业者）
     */
    private int role;
    /**
     * 户籍地址，身份证上地址（OCR获取）
     */
    private String	id_card_address;
    /**
     * 民族(如：汉，OCR获取)
     */
    private String	nation;
    /**
     * 身份证正面图片URL
     */
    private String	id_positive;
    /**
     * 身份证反面图片URL
     */
    private String	id_negative;
    /**
     * 人脸照片URL
     */
    private String	face;
    /**
     * 身份证签发机构（身份证上签发机构，OCR获取）
     */
    private String	id_signing_authority;
    /**
     * 身份证有效截止日期(格式：2036-09-01或长期，OCR获取)
     */
    private String	id_expiry_date;
    /**
     * 身份证有效起始日期(格式：2016-09-01，OCR获取)
     */
    private String	id_start_date;
    /**
     * 身份证正面有效真实度： ID Photo （正式身份证照片） Temporary ID Photo （临时身份证照片） Photocopy （正式身份证的复印件） Screen （手机或电脑屏幕翻拍的照片） Edited （用工具合成或者编辑过的身份证图片）
     */
    private String	id_positive_valid_state;
    /**
     * 身份证反面有效真实度： ID Photo （正式身份证照片） Temporary ID Photo （临时身份证照片） Photocopy （正式身份证的复印件） Screen （手机或电脑屏幕翻拍的照片） Edited （用工具合成或者编辑过的身份证图片）
     */
    private String id_negative_valid_state;

    public String getUser_name() {
        return user_name;
    }

    public void setUser_name(String user_name) {
        this.user_name = user_name;
    }

    public String getPhone() {
        return phone;
    }

    public void setPhone(String phone) {
        this.phone = phone;
    }

    public String getId_card() {
        return id_card;
    }

    public void setId_card(String id_card) {
        this.id_card = id_card;
    }

    public String getGender() {
        return gender;
    }

    public void setGender(String gender) {
        this.gender = gender;
    }

    public int getRole() {
        return role;
    }

    public void setRole(int role) {
        this.role = role;
    }

    public String getId_card_address() {
        return id_card_address;
    }

    public void setId_card_address(String id_card_address) {
        this.id_card_address = id_card_address;
    }

    public String getNation() {
        return nation;
    }

    public void setNation(String nation) {
        this.nation = nation;
    }

    public String getId_positive() {
        return id_positive;
    }

    public void setId_positive(String id_positive) {
        this.id_positive = id_positive;
    }

    public String getId_negative() {
        return id_negative;
    }

    public void setId_negative(String id_negative) {
        this.id_negative = id_negative;
    }

    public String getFace() {
        return face;
    }

    public void setFace(String face) {
        this.face = face;
    }

    public String getId_signing_authority() {
        return id_signing_authority;
    }

    public void setId_signing_authority(String id_signing_authority) {
        this.id_signing_authority = id_signing_authority;
    }

    public String getId_expiry_date() {
        return id_expiry_date;
    }

    public void setId_expiry_date(String id_expiry_date) {
        this.id_expiry_date = id_expiry_date;
    }

    public String getId_start_date() {
        return id_start_date;
    }

    public void setId_start_date(String id_start_date) {
        this.id_start_date = id_start_date;
    }

    public String getId_positive_valid_state() {
        return id_positive_valid_state;
    }

    public void setId_positive_valid_state(String id_positive_valid_state) {
        this.id_positive_valid_state = id_positive_valid_state;
    }

    public String getId_negative_valid_state() {
        return id_negative_valid_state;
    }

    public void setId_negative_valid_state(String id_negative_valid_state) {
        this.id_negative_valid_state = id_negative_valid_state;
    }

    @Override
    public String toString() {
        final StringBuilder sb = new StringBuilder("UserInfoDto{");
        sb.append("user_name=").append(user_name);
        sb.append(", phone=").append(phone);
        sb.append(", id_card=").append(id_card);
        sb.append(", gender=").append(gender);
        sb.append(", role=").append(role);
        sb.append(", id_card_address=").append(id_card_address);
        sb.append(", nation=").append(nation);
        sb.append(", id_positive=").append(id_positive);
        sb.append(", id_negative=").append(id_negative);
        sb.append(", face=").append(face);
        sb.append(", id_signing_authority=").append(id_signing_authority);
        sb.append(", id_expiry_date=").append(id_expiry_date);
        sb.append(", id_start_date=").append(id_start_date);
        sb.append(", id_positive_valid_state=").append(id_positive_valid_state);
        sb.append(", id_negative_valid_state=").append(id_negative_valid_state);
        sb.append('}');
        return sb.toString();
    }
}
