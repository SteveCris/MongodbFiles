package com.shangyong.thcore.vo;

import java.io.Serializable;
import java.math.BigDecimal;

public class OrderBorrowVo implements Serializable {



	/**
	 * 
	 */
	private static final long serialVersionUID = 3486288444586252194L;

	// 期数
	private Integer periods;

	// 每期天数
	private Integer cycle;

	// 借款金额
	private BigDecimal borrowAmount;

	// 前置借款金额
	private BigDecimal preBorrowAmount;

	public Integer getPeriods() {
		return periods;
	}

	public void setPeriods(Integer periods) {
		this.periods = periods;
	}

	public Integer getCycle() {
		return cycle;
	}

	public void setCycle(Integer cycle) {
		this.cycle = cycle;
	}

	public BigDecimal getBorrowAmount() {
		return borrowAmount;
	}

	public void setBorrowAmount(BigDecimal borrowAmount) {
		this.borrowAmount = borrowAmount;
	}

	public BigDecimal getPreBorrowAmount() {
		return preBorrowAmount;
	}

	public void setPreBorrowAmount(BigDecimal preBorrowAmount) {
		this.preBorrowAmount = preBorrowAmount;
	}

	@Override
	public String toString() {
		StringBuilder builder = new StringBuilder();
		builder.append("OrderBorrowVo [periods=");
		builder.append(periods);
		builder.append(", cycle=");
		builder.append(cycle);
		builder.append(", borrowAmount=");
		builder.append(borrowAmount);
		builder.append(", preBorrowAmount=");
		builder.append(preBorrowAmount);
		builder.append("]");
		return builder.toString();
	}



}
