package com.shangyong.thorder.utils;

public class ConfigUtil {
	
	private ConfigUtil() {
		
	}

	public static String getEquityUrl() {
		return PropertiesUtil.get("equity.url");
	}
	
	public static String getSignkey() {
		return PropertiesUtil.get("equity.signkey");
	}
	
	public static String getServiceid() {
		return PropertiesUtil.get("equity.serviceid");
	}
	
	public static String getCpno() {
		return PropertiesUtil.get("equity.cpno");
	}
	
	public static String getPgno() {
		return PropertiesUtil.get("equity.pgno");
	}
}
